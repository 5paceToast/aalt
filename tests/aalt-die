#! /usr/bin/env atf-sh

atf_test_case multi_word
multi_word_body() {
	atf_check \
		-s 'exit:1' \
		-o 'inline:[FATAL] multi word message\n' \
		/usr/bin/zsh -c 'autoload '$(atf_get_srcdir)'/../lib/*(.N); fpath+=( '$(atf_get_srcdir)'/../lib ); aalt-die multi word message'
}

atf_init_test_cases() {
	atf_add_test_case multi_word
}
